# --- SwiftyCare Web — Environment Variables ---
# Copy this file to .env and fill in real values:
#   cp .env.example .env

# API base URL (optional)
# Leave empty for local dev (Vite proxy handles /api → localhost:3001)
# Set for production/tunnel deployments:
#   VITE_API_BASE_URL=https://api.swifty-care.com
VITE_API_BASE_URL=

# Doctor login passcode (optional, default: "swiftycare")
VITE_DOCTOR_PASSCODE=swiftycare
